{
  "name": "agb-ai-team-system",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "type": "module",
  "scripts": {
    "lint": "node tools/ai-team/gates/lint.mjs",
    "test": "node --test",
    "build": "node tools/ai-team/gates/build.mjs",
    "contract:check": "python3 tools/ai-team/gates/contract_check.py",
    "migration:check": "python3 tools/ai-team/gates/migration_check.py",
    "state:validate": "python3 -m jsonschema -i docs/ops/ai-team-state.json docs/ops/schema/ai-team-state.schema.json",
    "config:validate": "python3 -m jsonschema -i docs/ops/ai-team-config.json docs/ops/schema/ai-team-config.schema.json"
  }
}

